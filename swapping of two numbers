#include<stdio.h>
int main()
{
    int a,b,t;
    printf("Enter the first number:");
    scanf("%d",&a);
    printf("Enter the second number:");
    scanf("%d",&b);
    printf("\nBefore swapping: ");
    printf("%d\t%d",a,b);
    t=a;
    a=b;
    b=t;
    printf("\nAfter swapping: ");
    printf("%d\t%d",a,b);
    return 0;
}
