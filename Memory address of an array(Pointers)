#include<stdio.h>
int main()
{
    int a[10],n;
    printf("Enter the number of elements in the array:");
    scanf("%d",&n);
    printf("Enter the elements:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int *ptr=&a[10];
    printf("The memory address of the array elements are:\n");
    for(int i=0;i<n;i++)
    {
        printf("\n%p",&a);
    }
    return 0;
}
