#include<stdio.h>
int main()
{
    int a[100],n;
    printf("Enter the number of elements in the array:");
    scanf("%d",&n);
    printf("Enter the numbers:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("The array elements are:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
