#include<stdio.h>
#include<math.h>
int main()
{
    int n,r,sum=0,temp;
    printf("Number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        r=n%10;
        sum=sum+pow(r,3);
        n=n/10;
    }
    if(temp==sum)
    {
        printf("\nThe given number is a Armstrong number");
    }
    else
    {
        printf("\nThe given number is not a Armstrong number");
    }
    return 0;
}
