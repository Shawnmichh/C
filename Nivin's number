#include<stdio.h>
int main()
{
    int n,r,sum=0,temp;
    printf("Number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        r=n%10;
        sum=sum+r;
        n=n/10;
    }
    printf("The sum:%d",sum);
    if(temp%sum==0)
    {
        printf("\nThe given number is Nivin's number");
    }
    else
    {
        printf("\nThe given number is not Nivin's number");
    }
    return 0;
}
