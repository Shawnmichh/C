#include<stdio.h>
#include<string.h>
int main()
{
    char str[100], sstr[100];
    int n;
    printf("\nEnter the string:");
    scanf("%s", &str);
    printf("\nEnter the number of char to copy:");
    scanf("%d",&n);
    int i=0;
    while(str[i]!='\0' && i<n)
    {
        sstr[i]=str[i];
        i++;
    }
    sstr[i]='\0';
    printf("\nThe substring is:");
    puts(sstr);
    return 0;
}
