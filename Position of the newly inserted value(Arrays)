#include<stdio.h>
int main()
{
    int a[100],n,pos;
    printf("Enter the number of elements in the array:");
    scanf("%d",&n);
    printf("Enter the numbers:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
   printf("Enter the position to insert:");
   scanf("%d",&pos);
    for(int i=n-1;i>=pos;i--)
    {
        a[i+1]=a[i];
    }
    a[pos]=n;
    n++;
    printf("\nAfter insertion:%d",n);
    for(int i=0;i<n;i++)
    {
        printf("\n%d",a[i]);
    }
    return 0;
}
