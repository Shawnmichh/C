#include<stdio.h>
int main()
{
    int n,m,sum=0;
    printf("Enter the first number:");
    scanf("%d",&n);
    printf("Enter the end number:");
    scanf("%d",&m);
    while(n<=m)
    {
       
       if(n%2==0)
       {
           sum+=n;
       }
        n++;
    }
    printf("The sum is %d",sum);
    return 0;
}
