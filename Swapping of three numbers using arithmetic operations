#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    printf("Before swapping:\n");
    printf("\n%d",a);
    printf("\n%d",b);
    printf("\n%d",c);
    a=a+b+c;
    b=a-(b+c);
    c=a-(b+c);
    a=a-(b+c);
    printf("After swapping:\n");
    printf("\n%d",a);
    printf("\n%d",b);
    printf("\n%d",c);
    return 0;
}
